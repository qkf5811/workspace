package com.dkt.school.sch.dto;

import com.dkt.school.sch.model.MajorField;

public class MajorFieldDto extends MajorField {
	
	private String majorStr;

	private Integer classCount;
	
	private Integer majorFieldStuCount;
	
	public Integer getClassCount() {
		return classCount;
	}
	public void setClassCount(Integer classCount) {
		this.classCount = classCount;
	}
	public Integer getMajorFieldStuCount() {
		return majorFieldStuCount;
	}
	public void setMajorFieldStuCount(Integer majorFieldStuCount) {
		this.majorFieldStuCount = majorFieldStuCount;
	}
	public String getMajorStr() {
		return majorStr;
	}

	public void setMajorStr(String majorStr) {
		this.majorStr = majorStr;
	}
}
