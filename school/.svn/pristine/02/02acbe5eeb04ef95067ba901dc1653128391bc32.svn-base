package com.dkt.school.edu.dto;

import com.dkt.school.edu.model.CourseSchedule;


public class CourseScheduleDto extends CourseSchedule {
	private String scoreTypeStr;		//分数类型
		
	private String examDateStr;			//考试时间
	
	private String courseName;			//课程名
			
	private String schoolYearId;		//学年
	
	private String termTypeStr;			//学期
	
	private String teacherName;			//教师名
	
	private String majorStr;			//专业
	
	private String fieldStr;			//方向
	
	private String grade;				//年级
	
	private String classes;				//班级
	
	private String courseTypeStr;		//课程类型
	
	private String examineTypeStr;		//考试类型
	
	private String classroom;			//教室
	
	private Integer hours;				//学时
	
	private Float credit;				//学分

	public Float getCredit() {
		return credit;
	}

	public void setCredit(Float credit) {
		this.credit = credit;
	}

	public Integer getHours() {
		return hours;
	}

	public void setHours(Integer hours) {
		this.hours = hours;
	}

	public String getScoreTypeStr() {
		return scoreTypeStr;
	}

	public void setScoreTypeStr(String scoreTypeStr) {
		this.scoreTypeStr = scoreTypeStr;
	}

	public String getExamDateStr() {
		return examDateStr;
	}

	public void setExamDateStr(String examDateStr) {
		this.examDateStr = examDateStr;
	}

	public String getCourseName() {
		return courseName;
	}

	public void setCourseName(String courseName) {
		this.courseName = courseName;
	}

	public String getSchoolYearId() {
		return schoolYearId;
	}

	public void setSchoolYearId(String schoolYearId) {
		this.schoolYearId = schoolYearId;
	}

	public String getTermTypeStr() {
		return termTypeStr;
	}

	public void setTermTypeStr(String termTypeStr) {
		this.termTypeStr = termTypeStr;
	}

	public String getTeacherName() {
		return teacherName;
	}

	public void setTeacherName(String teacherName) {
		this.teacherName = teacherName;
	}

	public String getMajorStr() {
		return majorStr;
	}

	public void setMajorStr(String majorStr) {
		this.majorStr = majorStr;
	}

	public String getFieldStr() {
		return fieldStr;
	}

	public void setFieldStr(String fieldStr) {
		this.fieldStr = fieldStr;
	}
	
	public String getGrade() {
		return grade;
	}

	public void setGrade(String grade) {
		this.grade = grade;
	}
	
	public String getClasses() {
		return classes;
	}

	public void setClasses(String classes) {
		this.classes = classes;
	}

	public String getCourseTypeStr() {
		return courseTypeStr;
	}

	public void setCourseTypeStr(String courseTypeStr) {
		this.courseTypeStr = courseTypeStr;
	}

	public String getExamineTypeStr() {
		return examineTypeStr;
	}

	public void setExamineTypeStr(String examineTypeStr) {
		this.examineTypeStr = examineTypeStr;
	}

	public String getClassroom() {
		return classroom;
	}

	public void setClassroom(String classroom) {
		this.classroom = classroom;
	}

	@Override
	public String toString() {
		return "CourseScheduleDto [scoreTypeStr=" + scoreTypeStr
				+ ", examDateStr=" + examDateStr + ", courseName=" + courseName
				+ ", schoolYearId=" + schoolYearId + ", termTypeStr="
				+ termTypeStr + ", teacherName=" + teacherName + ", majorStr="
				+ majorStr + ", fieldStr=" + fieldStr + ", grade=" + grade
				+ ", classes=" + classes + ", courseTypeStr=" + courseTypeStr
				+ ", examineTypeStr=" + examineTypeStr + ", classroom="
				+ classroom + ", hours=" + hours + ", credit=" + credit
				+ ", getWeekDay()=" + getWeekDay() + ", getSectionType()="
				+ getSectionType() + ", getWeekIds()=" + getWeekIds()
				+ ", getWeeks()=" + getWeeks() + "]";
	}


	
	
}
