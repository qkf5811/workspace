package com.dkt.school.fin.dto;

import com.dkt.school.fin.model.ChargeStandard;

/**
 * Created with com.dkt.school.fin.dto by Harry on 2015/8/27 15:14.
 */
public class ChargeStandardDto extends ChargeStandard{

    private String gradeName;
    private String termTypeString;
    private String majorName;
    private String majorFieldName;

    public String getGradeName() {
        return gradeName;
    }

    public void setGradeName(String gradeName) {
        this.gradeName = gradeName;
    }

    public String getTermTypeString() {
        return termTypeString;
    }

    public void setTermTypeString(String termTypeString) {
        this.termTypeString = termTypeString;
    }

    public String getMajorName() {
        return majorName;
    }

    public void setMajorName(String majorName) {
        this.majorName = majorName;
    }

    public String getMajorFieldName() {
        return majorFieldName;
    }

    public void setMajorFieldName(String majorFieldName) {
        this.majorFieldName = majorFieldName;
    }
}
