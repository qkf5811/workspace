package com.dkt.school.stu.model;

import java.util.Date;

public class StudentFamily {
    private Integer id;

    private String name;

    private Integer relationshipType;

    private Date birthday;

    private String politicalFeatureType;

    private Integer workUnit;

    private String remark;

    public Integer getId() {
        return id;
    }

    public void setId(Integer id) {
        this.id = id;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public Integer getRelationshipType() {
        return relationshipType;
    }

    public void setRelationshipType(Integer relationshipType) {
        this.relationshipType = relationshipType;
    }

    public Date getBirthday() {
        return birthday;
    }

    public void setBirthday(Date birthday) {
        this.birthday = birthday;
    }

    public String getPoliticalFeatureType() {
        return politicalFeatureType;
    }

    public void setPoliticalFeatureType(String politicalFeatureType) {
        this.politicalFeatureType = politicalFeatureType;
    }

    public Integer getWorkUnit() {
        return workUnit;
    }

    public void setWorkUnit(Integer workUnit) {
        this.workUnit = workUnit;
    }

    public String getRemark() {
        return remark;
    }

    public void setRemark(String remark) {
        this.remark = remark;
    }
}