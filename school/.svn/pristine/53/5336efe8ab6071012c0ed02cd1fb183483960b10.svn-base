package com.dkt.school.tea.model;

import java.util.Date;

public class TeacherPunishment {
    private Integer id;

    private Integer teacherId;

    private Integer punishmentType;

    private String title;

    private String content;

    private Integer schoolYearId;

    private Integer schoolTermType;

    private Date punishDate;

    private String remark;

    public Integer getId() {
        return id;
    }

    public void setId(Integer id) {
        this.id = id;
    }

    public Integer getTeacherId() {
        return teacherId;
    }

    public void setTeacherId(Integer teacherId) {
        this.teacherId = teacherId;
    }

    public Integer getPunishmentType() {
        return punishmentType;
    }

    public void setPunishmentType(Integer punishmentType) {
        this.punishmentType = punishmentType;
    }

    public String getTitle() {
        return title;
    }

    public void setTitle(String title) {
        this.title = title;
    }

    public String getContent() {
        return content;
    }

    public void setContent(String content) {
        this.content = content;
    }

    public Integer getSchoolYearId() {
        return schoolYearId;
    }

    public void setSchoolYearId(Integer schoolYearId) {
        this.schoolYearId = schoolYearId;
    }

    public Integer getSchoolTermType() {
        return schoolTermType;
    }

    public void setSchoolTermType(Integer schoolTermType) {
        this.schoolTermType = schoolTermType;
    }

    public Date getPunishDate() {
        return punishDate;
    }

    public void setPunishDate(Date punishDate) {
        this.punishDate = punishDate;
    }

    public String getRemark() {
        return remark;
    }

    public void setRemark(String remark) {
        this.remark = remark;
    }

	@Override
	public String toString() {
		return "TeacherPunishment [id=" + id + ", teacherId=" + teacherId
				+ ", punishmentType=" + punishmentType + ", title=" + title
				+ ", content=" + content + ", schoolYearId=" + schoolYearId
				+ ", schoolTermType=" + schoolTermType + ", punishDate="
				+ punishDate + ", remark=" + remark + "]";
	}
    
    
}