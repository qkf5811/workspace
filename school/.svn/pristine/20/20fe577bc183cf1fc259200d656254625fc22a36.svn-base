package com.dkt.school.edu.dto;

import com.dkt.school.edu.model.Course;

public class CourseOfManualAdjustDto extends Course {
	
	private String teaName;
	
	private String majorIdStr;
	
	private String fieldIdStr;
	
	private String classes;
	
	private String courseTypeStr;
	
	private String examineTypeStr;

	public String getTeaName() {
		return teaName;
	}

	public void setTeaName(String teaName) {
		this.teaName = teaName;
	}

	public String getMajorIdStr() {
		return majorIdStr;
	}

	public void setMajorIdStr(String majorIdStr) {
		this.majorIdStr = majorIdStr;
	}

	public String getFieldIdStr() {
		return fieldIdStr;
	}

	public void setFieldIdStr(String fieldIdStr) {
		this.fieldIdStr = fieldIdStr;
	}

	public String getClasses() {
		return classes;
	}

	public void setClasses(String classes) {
		this.classes = classes;
	}

	public String getCourseTypeStr() {
		return courseTypeStr;
	}

	public void setCourseTypeStr(String courseTypeStr) {
		this.courseTypeStr = courseTypeStr;
	}

	public String getExamineTypeStr() {
		return examineTypeStr;
	}

	public void setExamineTypeStr(String examineTypeStr) {
		this.examineTypeStr = examineTypeStr;
	}
}
