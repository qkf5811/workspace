package com.dkt.school.edu.dto;

import com.dkt.school.edu.model.TeachTask;

public class CourseToQueryScoByHTDto extends TeachTask {

	private String courseCode;
	
	private String courseName;
	
	private String studyYearStr;
	
	private String termTypeStr;
	
	private String className;
	


	public String getClassName() {
		return className;
	}

	public void setClassName(String className) {
		this.className = className;
	}

	public String getCourseCode() {
		return courseCode;
	}

	public void setCourseCode(String courseCode) {
		this.courseCode = courseCode;
	}

	public String getCourseName() {
		return courseName;
	}

	public void setCourseName(String courseName) {
		this.courseName = courseName;
	}

	public String getStudyYearStr() {
		return studyYearStr;
	}

	public void setStudyYearStr(String studyYearStr) {
		this.studyYearStr = studyYearStr;
	}

	public String getTermTypeStr() {
		return termTypeStr;
	}

	public void setTermTypeStr(String termTypeStr) {
		this.termTypeStr = termTypeStr;
	}
	
}
