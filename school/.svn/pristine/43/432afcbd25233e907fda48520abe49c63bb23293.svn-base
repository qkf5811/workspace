package com.dkt.school.fin.model;

public class ChargeStandard {
    private Integer id;

    private Integer gradeId;

    private Integer termType;

    private Integer majorId;

    private Integer fieldId;

    private Float tuitionFee;

    private Float roomFeeGirl;

    private Float roomFeeBoy;

    private Float bookFee;

    public Integer getId() {
        return id;
    }

    public void setId(Integer id) {
        this.id = id;
    }

    public Integer getGradeId() {
        return gradeId;
    }

    public void setGradeId(Integer gradeId) {
        this.gradeId = gradeId;
    }

    public Integer getTermType() {
        return termType;
    }

    public void setTermType(Integer termType) {
        this.termType = termType;
    }

    public Integer getMajorId() {
        return majorId;
    }

    public void setMajorId(Integer majorId) {
        this.majorId = majorId;
    }

    public Integer getFieldId() {
        return fieldId;
    }

    public void setFieldId(Integer fieldId) {
        this.fieldId = fieldId;
    }

    public Float getTuitionFee() {
        return tuitionFee;
    }

    public void setTuitionFee(Float tuitionFee) {
        this.tuitionFee = tuitionFee;
    }

    public Float getRoomFeeGirl() {
        return roomFeeGirl;
    }

    public void setRoomFeeGirl(Float roomFeeGirl) {
        this.roomFeeGirl = roomFeeGirl;
    }

    public Float getRoomFeeBoy() {
        return roomFeeBoy;
    }

    public void setRoomFeeBoy(Float roomFeeBoy) {
        this.roomFeeBoy = roomFeeBoy;
    }

    public Float getBookFee() {
        return bookFee;
    }

    public void setBookFee(Float bookFee) {
        this.bookFee = bookFee;
    }
}