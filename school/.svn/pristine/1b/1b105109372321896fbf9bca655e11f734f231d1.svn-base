
#stuInfoDiv_list{
    margin-left: auto;
    margin-right: auto;
    width:800px;
 
}

#stuInfoDiv_list div{
    text-align:center;
}

#stuInfoDiv_list div span{
    margin-left:30px;
}

#edu_gridtable_list{
   margin-left:10%;
}

#edu_gridtable_list {
	font-family: verdana,arial,sans-serif;
	font-size:11px;
	color:#333333;
	border-width: 1px;
	border-collapse: collapse;
}
#edu_gridtable_list th {
	border-width: 1px;
	padding: 8px;
	border-style: solid;
	border-color: #666666;
	background-color:#DDDDDD;
}
#edu_gridtable_list td {
	border-width: 1px;
	padding: 8px;
	border-style: solid;
	border-color: #666666;
	font-size:12px;
}
#edu_gridtable_list tr{
    line-height:0.2;
}

table.gridtableInfo td{
    border-width: 0px;
	padding: 12px;
	border-style: solid;
	border-color: #666666;
}