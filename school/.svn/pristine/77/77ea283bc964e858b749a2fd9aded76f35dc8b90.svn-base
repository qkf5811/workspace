package com.dkt.school.equ.model;

import java.util.Date;

public class ReformRecord {
    private Integer id;

    private Integer itemId;

    private Integer reformUserId;

    private Date reformDate;

    private String remark;

    public Integer getId() {
        return id;
    }

    public void setId(Integer id) {
        this.id = id;
    }

    public Integer getItemId() {
        return itemId;
    }

    public void setItemId(Integer itemId) {
        this.itemId = itemId;
    }

    public Integer getReformUserId() {
        return reformUserId;
    }

    public void setReformUserId(Integer reformUserId) {
        this.reformUserId = reformUserId;
    }

    public Date getReformDate() {
        return reformDate;
    }

    public void setReformDate(Date reformDate) {
        this.reformDate = reformDate;
    }

    public String getRemark() {
        return remark;
    }

    public void setRemark(String remark) {
        this.remark = remark;
    }
}