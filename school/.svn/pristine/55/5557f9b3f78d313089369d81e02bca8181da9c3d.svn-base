package com.dkt.school.stu.model;

import java.util.Date;

public class StudentChange {
    private Integer id;

    private Integer studentId;

    private Integer changeType;

    private String cause;

    private Integer outClassId;

    private Date outClassDate;

    private Integer intoClassId;

    private Date intoClassDate;

    private String operUser;

    private String remark;

    public Integer getId() {
        return id;
    }

    public void setId(Integer id) {
        this.id = id;
    }

    public Integer getStudentId() {
        return studentId;
    }

    public void setStudentId(Integer studentId) {
        this.studentId = studentId;
    }

    public Integer getChangeType() {
        return changeType;
    }

    public void setChangeType(Integer changeType) {
        this.changeType = changeType;
    }

    public String getCause() {
        return cause;
    }

    public void setCause(String cause) {
        this.cause = cause;
    }

    public Integer getOutClassId() {
        return outClassId;
    }

    public void setOutClassId(Integer outClassId) {
        this.outClassId = outClassId;
    }

    public Date getOutClassDate() {
        return outClassDate;
    }

    public void setOutClassDate(Date outClassDate) {
        this.outClassDate = outClassDate;
    }

    public Integer getIntoClassId() {
        return intoClassId;
    }

    public void setIntoClassId(Integer intoClassId) {
        this.intoClassId = intoClassId;
    }

    public Date getIntoClassDate() {
        return intoClassDate;
    }

    public void setIntoClassDate(Date intoClassDate) {
        this.intoClassDate = intoClassDate;
    }

    public String getOperUser() {
        return operUser;
    }

    public void setOperUser(String operUser) {
        this.operUser = operUser;
    }

    public String getRemark() {
        return remark;
    }

    public void setRemark(String remark) {
        this.remark = remark;
    }
}